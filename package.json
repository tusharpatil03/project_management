    "@types/graphql-depth-limit": "^1.1.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
