    "@graphql-tools/utils": "^10.3.2",
    "@parcel/watcher": "^2.4.1",
    "@prisma/client": "^6.5.0",
    "@types/cors": "^2.8.17",
    "@types/graphql-upload": "^17.0.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-voyager": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
